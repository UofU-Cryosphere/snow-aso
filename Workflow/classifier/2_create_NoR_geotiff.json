{
  "pipeline": [
    "/Path/To/Source.laz",
    {
      "type": "filters.range",
      "limits": "UserData[3:3]"
    },
    {
      "type": "writers.gdal",
      "bounds": "([322521.0, 336552.0], [4305591.0, 4322445.0])",
      "filename": "/Path/To/Output.tif",
      "gdaldriver": "GTiff",
      "gdalopts": "TILED=YES,COMPRESS=LZW,BIGTIFF=IF_NEEDED,NUM_THREADS=ALL_CPUS,SPARSE_OK=TRUE",
      "output_type": "min,max",
      "dimension": "NumberOfReturns",
      "data_type": "uint8",
      "resolution": 3.0
    }
  ]
}
